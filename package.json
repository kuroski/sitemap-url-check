{
  "name": "sitemap-url-check",
  "type": "module",
  "packageManager": "pnpm@7.14.0",
  "private": false,
  "version": "1.0.0",
  "description": "Check your sitemap urls.",
  "author": "Daniel Kuroski <daniel.kuroski@gmail.com>",
  "keywords": [
    "typescript",
    "cli",
    "sitemap",
    "url",
    "checker"
  ],
  "license": "BSD-3-Clause",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "bin": {
    "sitemap-url-check": "dist/cli.mjs"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.27.0",
    "@antfu/ni": "0.18.2",
    "@antfu/utils": "0.6.0",
    "@types/node": "18.11.4",
    "@types/xml2js": "^0.4.11",
    "bumpp": "8.2.1",
    "eslint": "8.26.0",
    "esno": "0.16.3",
    "pkg": "5.8.0",
    "pnpm": "7.14.0",
    "rimraf": "3.0.2",
    "typescript": "4.8.4",
    "unbuild": "0.9.4",
    "vite": "3.1.8",
    "vitest": "0.24.3"
  },
  "dependencies": {
    "@supercharge/promise-pool": "^2.3.2",
    "commander": "9.4.1",
    "ohmyfetch": "^0.4.20",
    "ora": "^6.1.2",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "url-exists": "^1.0.3",
    "xml2js": "^0.4.23",
    "zod": "^3.19.1"
  }
}
